{
    "author"      : "Henri Bouvier",
    "name"        : "redisctl",
    "description" : "A redis client",
    "version"     : "0.0.1",
    "keywords"    : ["redis"],
    "repository"  : {
        "type" : "git",
        "url"  : "https://github.com/hbouvier/redisctl.git"
    },
    "dependencies"         : {
        "redisd"       : "0.0.5",
        "node-options" : "0.0.3",
        "winston"      : "0.7.2",
        "redis"        : "0.10.3"
    },
    "engines"              : {
        "node" : ">= 0.10.0 < 0.11.0"
    },
    "main" : "./lib/redisctl.js",
    "bin"  : {
        "redisd" : "./bin/redisctl"
    },
    "scripts": {
        "start": "bin/redisctl"
    },
    "configuration" : {
        "level"      : "error",
        "redis_urls" : "127.0.0.1:6379"
    }
}
